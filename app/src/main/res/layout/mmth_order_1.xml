<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/kiosk_white"
    tools:ignore="Orientation">

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/kiosk_white"
        app:layout_constraintTop_toTopOf="@+id/linearLayout8"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            android:gravity="center"
            android:text="매머드익스프레스"
            android:textColor="@color/kiosk_black_01"
            android:textSize="35sp"
            android:textStyle="bold" />
    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/kiosk_order_top"
        tools:ignore="MissingConstraints">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/textView15"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/kiosk_order_top"
        android:gravity="center"
        android:padding="10dp"
        android:text="매머드익스프레스"
        android:textColor="@color/kiosk_white"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:background="@color/kiosk_order_top"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginLeft="10dp"
            android:background="#00FF0000"
            android:src="@drawable/left_arrow"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginRight="15dp"
            android:background="#00FF0000"
            android:src="@drawable/right_arrow"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            app:columnCount="4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageButton2"
            app:layout_constraintStart_toEndOf="@+id/imageButton"
            app:layout_constraintTop_toTopOf="parent"
            app:rowCount="2">

            <TextView
                android:id="@+id/textview_1"
                android:layout_width="67dp"
                android:layout_height="17dp"
                android:layout_row="0"
                android:layout_rowSpan="1"
                android:layout_rowWeight="1"
                android:layout_column="1"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"

                android:gravity="center"
                android:text="신메뉴"
                android:textAlignment="center"
                android:textColor="@color/kiosk_white"
                android:textSize="8sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textview_2"
                android:layout_width="67dp"
                android:layout_height="17dp"
                android:layout_row="0"
                android:layout_rowWeight="1"
                android:layout_column="2"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:gravity="center"
                android:text="커피"
                android:textAlignment="center"
                android:textColor="@color/kiosk_white"
                android:textSize="8sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textview_3"
                android:layout_width="67dp"
                android:layout_height="17dp"
                android:layout_row="0"
                android:layout_rowSpan="1"
                android:layout_rowWeight="1"
                android:layout_column="3"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:gravity="center"
                android:text="디카페인"
                android:textAlignment="center"
                android:textColor="@color/kiosk_white"
                android:textSize="8sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textview_4"
                android:layout_width="67dp"
                android:layout_height="17dp"
                android:layout_row="0"
                android:layout_rowSpan="1"
                android:layout_rowWeight="1"
                android:layout_column="4"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@drawable/btn_select"
                android:gravity="center"
                android:text="콜드브루"
                android:textAlignment="center"
                android:textColor="@color/kiosk_black_01"
                android:textSize="8sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textview_5"
                android:layout_width="67dp"
                android:layout_height="10dp"
                android:layout_row="1"
                android:layout_rowSpan="1"
                android:layout_rowWeight="1"
                android:layout_column="1"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="1dp"
                android:gravity="center"
                android:text="논커피"
                android:textAlignment="center"
                android:textColor="@color/kiosk_white"
                android:textSize="8sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textview_6"
                android:layout_width="67dp"
                android:layout_height="17dp"
                android:layout_row="1"
                android:layout_rowSpan="1"
                android:layout_rowWeight="1"
                android:layout_column="2"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="1dp"
                android:gravity="center"
                android:text="티&amp;에이드"
                android:textAlignment="center"
                android:textColor="@color/kiosk_white"
                android:textSize="8sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textview_7"
                android:layout_width="67dp"
                android:layout_height="17dp"
                android:layout_row="1"
                android:layout_rowSpan="1"
                android:layout_rowWeight="1"
                android:layout_column="3"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="1dp"
                android:gravity="center"
                android:text="프라페&amp;블렌디드"
                android:textAlignment="center"
                android:textColor="@color/kiosk_white"
                android:textSize="8sp"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />

            <TextView
                android:id="@+id/textview_8"
                android:layout_width="67dp"
                android:layout_height="17dp"
                android:layout_row="1"
                android:layout_rowSpan="1"
                android:layout_rowWeight="1"
                android:layout_column="4"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="2dp"
                android:layout_marginRight="5dp"
                android:gravity="center"
                android:text="디카페인"
                android:textAlignment="center"
                android:textColor="@color/kiosk_white"
                android:textSize="8sp" />

        </GridLayout>

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            app:columnCount="4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageButton2"
            app:layout_constraintStart_toEndOf="@+id/imageButton"
            app:layout_constraintTop_toTopOf="parent"
            app:rowCount="2">

            <Button
                android:id="@+id/button_1"
                android:layout_width="67dp"
                android:layout_height="17dp"
                android:layout_row="0"
                android:layout_rowSpan="1"
                android:layout_rowWeight="1"
                android:layout_column="1"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:background="@drawable/btn_invisible" />

            <Button
                android:id="@+id/button_2"
                android:layout_width="67dp"
                android:layout_height="17dp"
                android:layout_row="0"
                android:layout_rowWeight="1"
                android:layout_column="2"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:background="@drawable/btn_invisible" />

            <Button
                android:id="@+id/button_3"
                android:layout_width="67dp"
                android:layout_height="17dp"
                android:layout_row="0"
                android:layout_rowSpan="1"
                android:layout_rowWeight="1"
                android:layout_column="3"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:background="@drawable/btn_invisible" />

            <Button
                android:id="@+id/button_4"
                android:layout_width="67dp"
                android:layout_height="17dp"
                android:layout_row="0"
                android:layout_rowSpan="1"
                android:layout_rowWeight="1"
                android:layout_column="4"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@drawable/btn_invisible" />

            <Button
                android:id="@+id/button_5"
                android:layout_width="67dp"
                android:layout_height="10dp"
                android:layout_row="1"
                android:layout_rowSpan="1"
                android:layout_rowWeight="1"
                android:layout_column="1"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="2dp"
                android:background="@drawable/btn_invisible" />

            <Button
                android:id="@+id/button_6"
                android:layout_width="67dp"
                android:layout_height="17dp"
                android:layout_row="1"
                android:layout_rowSpan="1"
                android:layout_rowWeight="1"
                android:layout_column="2"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="2dp"
                android:background="@drawable/btn_invisible" />

            <Button
                android:id="@+id/button_7"
                android:layout_width="67dp"
                android:layout_height="17dp"
                android:layout_row="1"
                android:layout_rowSpan="1"
                android:layout_rowWeight="1"
                android:layout_column="3"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="2dp"
                android:background="@drawable/btn_invisible" />

            <Button
                android:id="@+id/button_8"
                android:layout_width="67dp"
                android:layout_height="17dp"
                android:layout_row="1"
                android:layout_rowSpan="1"
                android:layout_rowWeight="1"
                android:layout_column="4"
                android:layout_columnWeight="1"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="2dp"
                android:layout_marginRight="5dp"
                android:background="@drawable/btn_invisible" />

        </GridLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout4"
        android:layout_width="413dp"
        android:layout_height="327dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3"
        tools:ignore="MissingConstraints">


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_cart_item"
        android:layout_width="254dp"
        android:layout_height="197dp"
        android:background="#D9D9D9"
        android:orientation="vertical"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout4"
        tools:ignore="MissingConstraints"
        tools:listitem="@layout/item_cart" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="159dp"
        android:layout_height="195dp"
        android:background="#D9D9D9"
        app:layout_constraintStart_toEndOf="@+id/rv_cart_item"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout4"
        tools:ignore="MissingConstraints">

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>